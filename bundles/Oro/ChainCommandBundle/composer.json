{
    "name": "oro/chaincommand-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for the Chain commands function.",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/yaml": "6.4.*"
    },

    "autoload": {
        "psr-4": {
            "Oro\\ChainCommandBundle\\": ""
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Oro\\ChainCommandBundle\\Tests\\": "tests"
        }
    },

    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
